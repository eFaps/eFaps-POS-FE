{
  "/api/*": {
    "target": "http://localhost:8080/",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/socket/*": {
    "target": "ws://localhost:8080/",
    "secure": false,
    "logLevel": "debug",
    "ws": true
  },
  "/extensions/": {
    "target": "http://localhost:4201/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "extensions/izipay-pinpad-element.js": "main.js",
      "extensions/niubiz-element.js": "main.js"
    }
  }
}
