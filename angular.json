{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pos": {
      "i18n": {
        "locales": {
          "es": {
            "translation": "src/locale/messages.es.xlf",
            "baseHref": ""
          }
        }
      },
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist"
            },
            "index": "src/index.html",
            "tsConfig": "tsconfig.app.json",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "preserveSymlinks": true,
            "assets": [{
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              }
            ],
            "styles": [{
                "input": "src/styles.scss"
              },
              {
                "inject": false,
                "input": "src/scss/azure-blue.scss",
                "bundleName": "azure-blue"
              },
              {
                "inject": false,
                "input": "src/scss/cyan-orange.scss",
                "bundleName": "cyan-orange"
              },
              {
                "inject": false,
                "input": "src/scss/magenta-violet.scss",
                "bundleName": "magenta-violet"
              },
              {
                "inject": false,
                "input": "src/scss/rose-red.scss",
                "bundleName": "rose-red"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules",
                "src",
                "src/scss"
              ]
            },
            "scripts": [],
            "fileReplacements": [{
                "replace": "src/app/util/translate-file-loader.ts",
                "with": "src/app/util/translate-file-loader.noop.ts"
              }
            ],
            "aot": false,
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [{
                "type": "anyComponentStyle",
                "maximumWarning": "6kb"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "fileReplacements": [{
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/util/translate-file-loader.ts",
                  "with": "src/app/util/translate-file-loader.noop.ts"
                }
              ]
            },
            "es": {
              "aot": true,
              "budgets": [{
                "type": "anyComponentStyle",
                "maximumWarning": "6kb"
              }],
              "localize": [
                "es"
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "pos:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "pos:build:production"
            },
            "es": {
              "buildTarget": "pos:build:es"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "pos:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.spec.json",
            "scripts": [

            ],
            "styles": [{
                "input": "src/styles.scss"
              },
              {
                "inject": false,
                "input": "src/scss/azure-blue.scss",
                "bundleName": "azure-blue"
              },
              {
                "inject": false,
                "input": "src/scss/cyan-orange.scss",
                "bundleName": "cyan-orange"
              },
              {
                "inject": false,
                "input": "src/scss/magenta-violet.scss",
                "bundleName": "magenta-violet"
              },
              {
                "inject": false,
                "input": "src/scss/rose-red.scss",
                "bundleName": "rose-red"
              }
            ],
            "assets": [{
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              }
            ],
            "fileReplacements": [{
                "replace": "src/app/util/translate-file-loader.ts",
                "with": "src/app/util/translate-file-loader.noop.ts"
              }
            ]
          }
        }
      }
    }
  },
  "cli": {},
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
