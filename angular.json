{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pos": {
      "i18n": {
        "locales": {
          "es": {
            "translation": "src/locale/messages.es.xlf",
            "baseHref": ""
          }
        }
      },
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "preserveSymlinks": true,
            "assets": [{
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              }
            ],
            "styles": [{
                "input": "src/styles.scss"
              },
              {
                "inject": false,
                "input": "src/scss/pink-bluegrey.scss",
                "bundleName": "pink-bluegrey"
              },
              {
                "inject": false,
                "input": "src/scss/deeppurple-amber.scss",
                "bundleName": "deeppurple-amber"
              },
              {
                "inject": false,
                "input": "src/scss/indigo-pink.scss",
                "bundleName": "indigo-pink"
              },
              {
                "inject": false,
                "input": "src/scss/purple-green.scss",
                "bundleName": "purple-green"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules",
                "src",
                "src/scss",
              ]
            },
            "scripts": [],
            "fileReplacements": [{
                "replace": "src/app/util/translate-file-loader.ts",
                "with": "src/app/util/translate-file-loader.noop.ts"
              },
              {
                "replace": "src/app/util/electron-util.ts",
                "with": "src/app/util/electron-util.noop.ts"
              }
            ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [{
                "type": "anyComponentStyle",
                "maximumWarning": "6kb"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [{
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/util/translate-file-loader.ts",
                  "with": "src/app/util/translate-file-loader.noop.ts"
                },
                {
                  "replace": "src/app/util/electron-util.ts",
                  "with": "src/app/util/electron-util.noop.ts"
                }
              ]
            },
            "es": {
              "aot": true,
              "budgets": [{
                "type": "anyComponentStyle",
                "maximumWarning": "6kb"
              }],
              "localize": [
                "es"
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "build-electron": {
          "builder": "@angular-guru/electron-builder:build",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [{
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              }
            ],
            "styles": [{
                "input": "src/styles.scss"
              },
              {
                "input": "src/scss/pink-bluegrey.scss",
                "lazy": true,
                "bundleName": "pink-bluegrey"
              },
              {
                "input": "src/scss/deeppurple-amber.scss",
                "lazy": true,
                "bundleName": "deeppurple-amber"
              },
              {
                "input": "src/scss/indigo-pink.scss",
                "lazy": true,
                "bundleName": "indigo-pink"
              },
              {
                "input": "src/scss/purple-green.scss",
                "lazy": true,
                "bundleName": "purple-green"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules",
                "src",
                "src/scss",
              ]
            },
            "scripts": [

            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.electron.prod.ts"
              }]
            },
            "development": {
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.electron.ts"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pos:build:production"
            },
            "es": {
              "browserTarget": "pos:build:es"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pos:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [

            ],
            "styles": [{
                "input": "src/styles.scss"
              },
              {
                "inject": false,
                "input": "src/scss/pink-bluegrey.scss",
                "bundleName": "pink-bluegrey"
              },
              {
                "inject": false,
                "input": "src/scss/deeppurple-amber.scss",
                "bundleName": "deeppurple-amber"
              },
              {
                "inject": false,
                "input": "src/scss/indigo-pink.scss",
                "bundleName": "indigo-pink"
              },
              {
                "inject": false,
                "input": "src/scss/purple-green.scss",
                "bundleName": "purple-green"
              }
            ],
            "assets": [{
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              }
            ],
            "fileReplacements": [{
                "replace": "src/app/util/translate-file-loader.ts",
                "with": "src/app/util/translate-file-loader.noop.ts"
              },
              {
                "replace": "src/app/util/electron-util.ts",
                "with": "src/app/util/electron-util.noop.ts"
              }
            ]
          }
        }
      }
    },
    "pos-e2e": {
      "root": "e2e",
      "projectType": "application",
      "cli": {},
      "schematics": {},
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "pos:serve"
          }
        }
      }
    }
  },
  "cli": {},
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
