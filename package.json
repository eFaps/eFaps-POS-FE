{
  "name": "pos",
  "version": "0.10.0",
  "license": "Apache 2.0",
  "author": {
    "name": "eFaps.org",
    "email": "information@eFaps.org"
  },
  "homepage": "www.eFaps.org",
  "main": "main.js",
  "scripts": {
    "_build-css": "node-sass --output src/assets src/scss",
    "_build": "tsc -p tsconfig-electron.json && node ./buildutil.js",
    "start": "npm run _build-css && ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "node ./buildutil.js && npm run _build-css && ng build --prod",
    "build2": "node ./buildutil.js && npm run _build-css && ng build --aot --source-map false",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-live": "npm run _build && npm run _build-css && concurrently \"npm run start\" \"electron . --dev --life\"",
    "electron-build-prod": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration production",
    "electron-build-dev": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration development",
    "electron-prod": "npm run electron-build-prod && electron .",
    "electron": "npm run electron-build-dev && electron . --dev",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^7.1.3",
    "@angular/compiler": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/router": "^7.1.3",
    "@ngx-material-keyboard/core": "^0.1.1",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@stomp/ng2-stompjs": "^7.0.0",
    "angular-svg-icon": "^7.0.1",
    "angular2-hotkeys": "^2.1.4",
    "core-js": "^2.6.0",
    "electron-browser-storage": "^1.0.6",
    "enum-values": "^1.2.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "ng-busy": "^6.0.0",
    "ngx-store": "^2.1.0",
    "parse-decimal-number": "^1.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.2",
    "@angular-guru/electron-builder": "^6.1.7",
    "@angular/cli": "^7.1.2",
    "@angular/compiler-cli": "^7.1.3",
    "@types/jasmine": "^3.3.1",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.12",
    "@types/sockjs-client": "^1.1.0",
    "@types/stompjs": "^2.3.4",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "electron": "^3.0.11",
    "electron-builder": "^20.38.3",
    "import-sort-style-module": "^5.0.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "^2.0.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-mocks": "^7.2.0",
    "node-sass": "^4.11.0",
    "protractor": "^5.4.1",
    "replace-in-file": "^3.4.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.1.6"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "build": {
    "appId": "efaps.pos.fe",
    "productName": "eFaps-POS-FE",
    "directories": {
      "output": "distribution"
    },
    "linux": {
      "category": "Office",
      "description": "POS",
      "target": [
        "AppImage",
        "deb",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
