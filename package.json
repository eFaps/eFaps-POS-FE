{
  "name": "pos",
  "version": "1.0.0",
  "license": "Apache 2.0",
  "author": {
    "name": "eFaps.org",
    "email": "information@eFaps.org"
  },
  "homepage": "www.eFaps.org",
  "main": "main.js",
  "scripts": {
    "_build": "tsc -p tsconfig-electron.json && node ./buildutil.js",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start-elements": "ng serve --live-reload false --proxy-config proxy.conf.json --host 0.0.0.0",
    "start-es": "ng serve --configuration es --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "ng build --configuration production --localize",
    "build2": "ng build --aot --source-map false",
    "test": "ng test --browsers ChromeHeadlessNoSandbox --no-progress --no-watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-live": "npm run _build && npm run _build-css && concurrently \"npm run start\" \"electron . --dev --life\"",
    "electron-build-prod": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration production",
    "electron-build-dev": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration development",
    "electron-prod": "npm run electron-build-prod && electron .",
    "electron": "npm run electron-build-dev && electron . --dev",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win",
    "i18n": "ng extract-i18n --output-path src/locale",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^14.1.0",
    "@angular/animations": "^14.1.3",
    "@angular/cdk": "^14.1.3",
    "@angular/common": "^14.1.3",
    "@angular/compiler": "^14.1.3",
    "@angular/core": "^14.1.3",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.3",
    "@angular/localize": "^14.1.3",
    "@angular/material": "^14.1.3",
    "@angular/platform-browser": "^14.1.3",
    "@angular/platform-browser-dynamic": "^14.1.3",
    "@angular/router": "^14.1.3",
    "@efaps/ngx-store": "^5.0.0",
    "@efaps/pos-library": "^6.0.2",
    "@ngx-pwa/local-storage": "^13.0.5",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@stomp/ng2-stompjs": "^8.0.0",
    "ajv-keywords": "^5.1.0",
    "angular-onscreen-keyboard-new": "0.2.0",
    "angular-svg-icon": "^14.0.0",
    "angular2-hotkeys": "^13.1.0",
    "decimal.js": "^10.3.1",
    "electron-browser-storage": "^1.0.7",
    "enum-values": "^1.2.1",
    "just-clone": "^6.1.1",
    "jwt-decode": "^3.1.2",
    "ng-busy": "^14.0.0",
    "ng-event-source": "^1.0.14",
    "parse-decimal-number": "^1.0.0",
    "rxjs": "^7.4.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "^14.1.3",
    "@angular/compiler-cli": "^14.1.3",
    "@types/core-js": "^2.5.5",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/mousetrap": "1.6.9",
    "@types/node": "^18.7.8",
    "@types/sockjs-client": "^1.5.1",
    "@types/stompjs": "^2.3.5",
    "codelyzer": "^6.0.2",
    "concurrently": "^5.3.0",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-mocks": "^14.1.3",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "replace-in-file": "^6.1.0",
    "sass": "^1.42.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~4.7.4"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "build": {
    "appId": "efaps.pos.fe",
    "productName": "eFaps-POS-FE",
    "directories": {
      "output": "distribution"
    },
    "linux": {
      "category": "Office",
      "description": "POS",
      "target": [
        "AppImage",
        "deb",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}