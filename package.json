{
  "name": "pos",
  "version": "1.0.0",
  "license": "Apache 2.0",
  "author": {
    "name": "eFaps.org",
    "email": "information@eFaps.org"
  },
  "homepage": "www.eFaps.org",
  "main": "main.js",
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start-elements": "ng serve --live-reload false --proxy-config proxy.conf.json --host 0.0.0.0",
    "start-es": "ng serve --configuration es --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "ng build --configuration production --localize",
    "build2": "ng build --aot --source-map false",
    "test": "ng test --browsers ChromeHeadlessNoSandbox --no-progress --no-watch",
    "lint": "ng lint",
    "i18n": "ng extract-i18n --output-path src/locale",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^15.3.0",
    "@angular/animations": "^17.3.3",
    "@angular/cdk": "^17.3.3",
    "@angular/common": "^17.3.3",
    "@angular/compiler": "^17.3.3",
    "@angular/core": "^17.3.3",
    "@angular/forms": "^17.3.3",
    "@angular/localize": "^17.3.3",
    "@angular/material": "^17.3.3",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.3.3",
    "@angular/router": "^17.3.3",
    "@efaps/angular-onscreen-material-keyboard": "^0.17.0",
    "@efaps/ngx-store": "^8.0.0",
    "@efaps/pos-library": "^9.3.1",
    "@ngx-pwa/local-storage": "^17.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@stomp/rx-stomp": "^1.2.0",
    "ajv-keywords": "^5.1.0",
    "angular-svg-icon": "^17.0.0",
    "angular2-hotkeys": "^16.0.1",
    "decimal.js": "^10.4.3",
    "enum-values": "^1.2.1",
    "just-clone": "^6.2.0",
    "jwt-decode": "^3.1.2",
    "material-icons": "^1.13.12",
    "ng-event-source": "^1.0.14",
    "parse-decimal-number": "^1.0.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^7.8.1",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.0.5",
    "@types/core-js": "^2.5.8",
    "@types/jasmine": "^4.6.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/mousetrap": "1.6.9",
    "@types/node": "^18.19.29",
    "@types/sockjs-client": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "codelyzer": "^6.0.2",
    "eslint": "^8.57.0",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-mocks": "^14.12.1",
    "prettier": "^2.8.8",
    "replace-in-file": "^6.3.5",
    "sass": "^1.74.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "build": {
    "appId": "efaps.pos.fe",
    "productName": "eFaps-POS-FE",
    "directories": {
      "output": "distribution"
    },
    "linux": {
      "category": "Office",
      "description": "POS",
      "target": [
        "AppImage",
        "deb",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
