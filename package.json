{
  "name": "pos",
  "version": "1.0.0",
  "license": "Apache 2.0",
  "author": {
    "name": "eFaps.org",
    "email": "information@eFaps.org"
  },
  "homepage": "www.eFaps.org",
  "main": "main.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "_build": "tsc -p tsconfig-electron.json && node ./buildutil.js",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start-es": "ng serve --configuration es --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "node ./buildutil.js && ng build --configuration production --localize",
    "build2": "node ./buildutil.js && npm run _build-css && ng build --aot --source-map false",
    "test": "ng test --browsers ChromeHeadlessNoSandbox --no-progress --no-watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-live": "npm run _build && npm run _build-css && concurrently \"npm run start\" \"electron . --dev --life\"",
    "electron-build-prod": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration production",
    "electron-build-dev": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration development",
    "electron-prod": "npm run electron-build-prod && electron .",
    "electron": "npm run electron-build-dev && electron . --dev",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win",
    "i18n": "ng xi18n --output-path locale",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^11.0.1",
    "@angular/animations": "^12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.1.4",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.2.0",
    "@angular/localize": "^12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@efaps/ngx-store": "^4.0.8",
    "@efaps/pos-library": "^4.9.11",
    "@giakki/angular2-hotkeys": "^2.2.1",
    "@ngx-pwa/local-storage": "^12.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@stomp/ng2-stompjs": "^8.0.0",
    "angular-onscreen-material-keyboard": "^0.5.1",
    "angular-svg-icon": "^11.2.0",
    "decimal.js": "^10.3.1",
    "electron-browser-storage": "^1.0.7",
    "enum-values": "^1.2.1",
    "jwt-decode": "^3.1.2",
    "ng-busy": "^11.0.0",
    "ng-event-source": "^1.0.14",
    "parse-decimal-number": "^1.0.0",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.0",
    "@angular-guru/electron-builder": "^8.0.0",
    "@angular/cli": "^12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@types/core-js": "^2.5.4",
    "@types/jasmine": "^3.8.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/mousetrap": "1.6.3",
    "@types/node": "^14.17.9",
    "@types/sockjs-client": "^1.5.1",
    "@types/stompjs": "^2.3.5",
    "codelyzer": "^6.0.2",
    "concurrently": "^5.3.0",
    "electron": "^11.4.11",
    "electron-builder": "^22.11.7",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-mocks": "^11.11.2",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "replace-in-file": "^6.1.0",
    "sass": "^1.37.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "~4.3.5"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "build": {
    "appId": "efaps.pos.fe",
    "productName": "eFaps-POS-FE",
    "directories": {
      "output": "distribution"
    },
    "linux": {
      "category": "Office",
      "description": "POS",
      "target": [
        "AppImage",
        "deb",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "resolutions": {
    "@angular/core": "^12.1.3"
  }
}
