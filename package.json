{
  "name": "pos",
  "version": "1.0.0",
  "license": "Apache 2.0",
  "author": {
    "name": "eFaps.org",
    "email": "information@eFaps.org"
  },
  "homepage": "www.eFaps.org",
  "main": "main.js",
  "scripts": {
    "_build-css": "node-sass --output src/assets src/scss",
    "_build": "tsc -p tsconfig-electron.json && node ./buildutil.js",
    "start": "npm run _build-css && ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start-es": "npm run _build-css && ng serve --configuration es --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "node ./buildutil.js && npm run _build-css && ng build --prod --localize",
    "build2": "node ./buildutil.js && npm run _build-css && ng build --aot --source-map false",
    "test": "ng test --browsers ChromeHeadlessNoSandbox --no-progress --no-watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-live": "npm run _build && npm run _build-css && concurrently \"npm run start\" \"electron . --dev --life\"",
    "electron-build-prod": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration production",
    "electron-build-dev": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration development",
    "electron-prod": "npm run electron-build-prod && electron .",
    "electron": "npm run electron-build-dev && electron . --dev",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win",
    "i18n": "ng xi18n --output-path locale",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.0.14",
    "@angular/localize": "^10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@efaps/ngx-store": "^4.0.0",
    "@efaps/pos-library": "^2.1.4",
    "@ngx-pwa/local-storage": "^10.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@stomp/ng2-stompjs": "^7.2.0",
    "angular-onscreen-material-keyboard": "^0.4.0",
    "angular-svg-icon": "^10.0.0",
    "angular2-hotkeys": "^2.2.0",
    "decimal.js": "^10.2.0",
    "electron-browser-storage": "^1.0.6",
    "enum-values": "^1.2.1",
    "jwt-decode": "^2.2.0",
    "ng-busy": "^9.0.0",
    "ng-event-source": "^1.0.14",
    "ngx-cacheable": "^1.4.2",
    "parse-decimal-number": "^1.0.0",
    "rxjs": "^6.6.2",
    "rxjs-compat": "^6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.8",
    "@angular-guru/electron-builder": "^8.0.0",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@types/core-js": "^2.5.3",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.6.2",
    "@types/sockjs-client": "^1.1.1",
    "@types/stompjs": "^2.3.4",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^10.1.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "protractor": "~7.0.0",
    "replace-in-file": "^6.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "build": {
    "appId": "efaps.pos.fe",
    "productName": "eFaps-POS-FE",
    "directories": {
      "output": "distribution"
    },
    "linux": {
      "category": "Office",
      "description": "POS",
      "target": [
        "AppImage",
        "deb",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
