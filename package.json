{
  "name": "pos",
  "version": "0.9.0",
  "license": "Apache 2.0",
  "homepage": "www.eFaps.org",
  "main": "main.js",
  "scripts": {
    "start": "npm run build-css && ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "node ./buildutil.js && npm run build-css && ng build --prod",
    "build2": "node ./buildutil.js && npm run build-css && ng build --aot --source-map false",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-css": "node-sass --output src/assets src/scss",
    "dev": "tsc -p tsconfig-electron.json && concurrently \"npm run start\" \"electron . --dev\"",
    "build-electron": "tsc -p tsconfig-electron.json && node ./buildutil.js && npm run build-css && ng run pos:build-electron --configuration production",
    "electron": "npm run build-electron && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.2",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^7.1.2",
    "@angular/compiler": "^7.1.2",
    "@angular/core": "^7.1.2",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.1.2",
    "@angular/http": "^7.1.2",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^7.1.2",
    "@angular/platform-browser-dynamic": "^7.1.2",
    "@angular/router": "^7.1.2",
    "@ngx-material-keyboard/core": "^0.1.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@stomp/ng2-stompjs": "^6.0.1",
    "angular-svg-icon": "^6.0.1",
    "angular2-hotkeys": "^2.1.4",
    "core-js": "^2.6.0",
    "enum-values": "^1.2.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "ng-busy": "^1.4.8",
    "ngx-store": "^2.1.0",
    "parse-decimal-number": "^1.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.7",
    "@angular-guru/electron-builder": "^6.1.7",
    "@angular/cli": "^7.1.2",
    "@angular/compiler-cli": "^7.1.2",
    "@types/jasmine": "^2.8.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^6.14.2",
    "@types/sockjs-client": "^1.1.0",
    "@types/stompjs": "^2.3.4",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "electron": "^3.0.10",
    "import-sort-style-module": "^4.2.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-mocks": "^7.2.0",
    "node-sass": "^4.11.0",
    "protractor": "^5.4.1",
    "replace-in-file": "^3.4.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.11.0",
    "typescript": "~3.1.6"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
