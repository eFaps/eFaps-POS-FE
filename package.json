{
  "name": "pos",
  "version": "1.0.0",
  "license": "Apache 2.0",
  "author": {
    "name": "eFaps.org",
    "email": "information@eFaps.org"
  },
  "homepage": "www.eFaps.org",
  "main": "main.js",
  "scripts": {
    "_build-css": "node-sass --output src/assets src/scss",
    "_build": "tsc -p tsconfig-electron.json && node ./buildutil.js",
    "start": "npm run _build-css && ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start-es": "npm run _build-css && ng serve --configuration es --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "node ./buildutil.js && npm run _build-css && ng build --prod",
    "build-es": "node ./buildutil.js && npm run _build-css && ng build --prod --build-optimizer --i18n-locale es --i18n-format xlf --i18n-file src/locale/messages.es.xlf ",
    "build2": "node ./buildutil.js && npm run _build-css && ng build --aot --source-map false",
    "test": "ng test --browsers ChromeHeadlessNoSandbox --no-progress --no-watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-live": "npm run _build && npm run _build-css && concurrently \"npm run start\" \"electron . --dev --life\"",
    "electron-build-prod": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration production",
    "electron-build-dev": "npm run _build && npm run _build-css && ng run pos:build-electron --configuration development",
    "electron-prod": "npm run electron-build-prod && electron .",
    "electron": "npm run electron-build-dev && electron . --dev",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win",
    "i18n": "ng xi18n --output-path locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.4",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/core": "^9.0.4",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^9.0.4",
    "@angular/localize": "~9.0.3",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/router": "^9.0.4",
    "@efaps/pos-library": "^0.1.0",
    "@ngx-material-keyboard/core": "^0.1.1",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@stomp/ng2-stompjs": "^7.2.0",
    "angular-svg-icon": "^7.2.1",
    "angular2-hotkeys": "^2.1.5",
    "core-js": "^2.6.11",
    "decimal.js": "^10.2.0",
    "electron-browser-storage": "^1.0.6",
    "enum-values": "^1.2.1",
    "jwt-decode": "^2.2.0",
    "ng-busy": "^8.0.0",
    "ng-event-source": "^1.0.14",
    "ngx-cacheable": "^1.4.0",
    "ngx-store": "^2.1.0",
    "parse-decimal-number": "^1.0.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-guru/electron-builder": "^7.2.20",
    "@angular/cli": "^9.0.4",
    "@angular/compiler-cli": "^9.0.4",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@types/sockjs-client": "^1.1.1",
    "@types/stompjs": "^2.3.4",
    "codelyzer": "^5.1.2",
    "concurrently": "^4.1.2",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "import-sort-style-module": "^5.0.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-mocks": "^8.1.0",
    "node-sass": "^4.13.1",
    "protractor": "^5.4.3",
    "replace-in-file": "^3.4.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "typescript": "~3.7.5"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "build": {
    "appId": "efaps.pos.fe",
    "productName": "eFaps-POS-FE",
    "directories": {
      "output": "distribution"
    },
    "linux": {
      "category": "Office",
      "description": "POS",
      "target": [
        "AppImage",
        "deb",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
